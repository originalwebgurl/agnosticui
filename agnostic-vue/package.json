{
  "name": "agnostic-vue",
  "version": "1.0.18",
  "description": "AgnosticUI (Vue)",
  "author": "Rob Levin <roblevinillustration@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AgnosticUI/agnosticui.git"
  },
  "keywords": [
    "agnosticui",
    "agnostic",
    "component",
    "components",
    "ui",
    "vue-components",
    "vue ui",
    "vue",
    "framework",
    "frontend"
  ],
  "main": "./dist/agnostic-vue.esm.js",
  "browser": "./dist/agnostic-vue.umd.js",
  "module": "./dist/agnostic-vue.esm.js",
  "files": [
    "src/**/*.{css,js,jsx,vue,ts,tsx}",
    "dist/**/*.{css,js,jsx,vue,ts,tsx}"
  ],
  "sideEffects": false,
  "scripts": {
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "cp:css": "cp ./src/css/*.css ./dist",
    "syncStyles": "node copystyles.js",
    "dev": "vite --port 3011",
    "build": "yarn syncStyles && vue-tsc --noEmit && vite build && yarn cp:css",
    "serve": "vite preview",
    "storybook": "yarn syncStyles && start-storybook -p 6006",
    "build-storybook": "yarn syncStyles && build-storybook --docs -o docs"
  },
  "dependencies": {
    "agnostic-helpers": "^1.0.2",
    "vue": "^3.2.31",
    "vue-a11y-dialog": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@rushstack/eslint-patch": "^1.1.0",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/vue3": "^6.3.12",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.25",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "babel-loader": "^8.2.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "typescript": "~4.5.5",
    "vite": "^2.8.4",
    "vitest": "^0.5.0",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.31.4"
  }
}
